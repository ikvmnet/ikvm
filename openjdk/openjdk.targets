    <Exec Command="peverify -nologo IKVM.OpenJDK.Nashorn.dll" />
    <Exec Command="peverify -nologo IKVM.OpenJDK.Remoting.dll" />
    <Exec Command="peverify -nologo IKVM.OpenJDK.Security.dll" />
