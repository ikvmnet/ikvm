<Project>
    <PropertyGroup>
        <LangVersion>7.0</LangVersion>
        <NoWarn>$(NoWarn);1591;1573;CS8002</NoWarn>
    </PropertyGroup>

    <PropertyGroup Label="Copyright Info">
        <ProductName>IKVM</ProductName>
        <Authors>Jeroen Frijters</Authors>
        <Company></Company>
        <CurrentYear Condition=" '$(CurrentYear)' == '' ">$([System.DateTime]::UtcNow.Year.ToString())</CurrentYear>
        <BeginCopyrightYear>2002-2015, 2018</BeginCopyrightYear>
        <CopyrightYearRange>$(BeginCopyrightYear)-$(CurrentYear)</CopyrightYearRange>
        <CopyrightYearRange Condition=" '$(BeginCopyrightYear)' == '$(CurrentYear)' ">$(CurrentYear)</CopyrightYearRange>
        <Copyright>Copyright Â© $(CopyrightYearRange) $(Company)</Copyright>
    </PropertyGroup>

    <PropertyGroup Label="Package Info">
        <RepositoryUrl>https://github.com/ikvm-revived/ikvm</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageProjectUrl>https://github.com/ikvm-revived/ikvm</PackageProjectUrl>
    </PropertyGroup>

    <PropertyGroup Label="Assembly Signing">
        <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)\ikvm.snk</AssemblyOriginatorKeyFile>
        <PublicKey></PublicKey>
        <SignAssembly Condition=" '$(AssemblyOriginatorKeyFile)' != '' ">true</SignAssembly>
    </PropertyGroup>

    <PropertyGroup Label="Version Info">
        <Version Condition=" '$(Version)' == '' ">0.0.0.0</Version>
        <AssemblyVersion Condition=" '$(AssemblyVersion)' == '' ">$(Version)</AssemblyVersion>
    </PropertyGroup>

    <PropertyGroup Label="Debug Info">
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

</Project>
